        Wait for : <span id="timer">20 </span>
        <tr></tr>
        Seconds to Continue
      </button>
      <button
        class="px-6 py-2 font-medium tracking-wide text-white capitalize transition-colors duration-300 transform bg-blue-600 rounded-lg hover:bg-blue-500 focus:outline-none focus:ring focus:ring-blue-300 focus:ring-opacity-80"
        id="continueButton"
        style="display: none"
      >
        Continue
      </button>
      <div id="scrlTxt" class="text-white" style="display: none">Scroll Down to Bottom And Click On Continue</div>
    </div>

    <script>
      // Get references to timer and continue button elements
    
      const timerBtn = document.getElementById("timerBtn");
      const timerElement = document.getElementById("timer");
      const continueButton = document.getElementById("continueButton");

      let countdown = 20;

      // Function to update timer display
      function updateTimerDisplay() {
        timerElement.textContent = countdown;
      }

      // Function to start the countdown
      function startCountdown() {
        updateTimerDisplay();

        // Countdown interval
        const interval = setInterval(() => {
          countdown--;

          if (countdown <= 0) {
            clearInterval(interval);
            timerElement.style.display = "none";
            timerBtn.style.display = "none";
            continueButton.style.display = "block";
            
          } else {
            updateTimerDisplay();
          }
        }, 1000);
      }

      // Add event listener to the continue button
      continueButton.addEventListener("click", () => {
        // Perform action when the button is clicked
        document.querySelector("#scrlTxt").style.display = "block"
        document.querySelector("#nextBtn").style.display = "block";
      });

      // Start the countdown when the page loads
      startCountdown();
    </script>

